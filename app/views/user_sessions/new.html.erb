<% form_for @user_session do |f| %>  
  <%= f.error_messages %> 
   
  <%= f.label :username, t("Username") %>
  <%= f.text_field :username %>  

  <%= f.label :password, t('Password') %>
  <%= f.password_field :password %>  

  <%= f.submit t("Login") %>  
<% end %> 




